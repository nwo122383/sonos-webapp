    "axios": "^1.7.5",
    "domparser": "^0.1.1",
    "postcss": "^8.4.49",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "react-swipeable": "^7.0.2",
    "react-touch-events": "^3.0.0",
    "xml2js": "^0.6.2",
    "xmldom": "^0.6.0"
